    <script>
        // Load cart items vào dropdown
        function loadCartItems() {
            $.ajax({
                url: '@Url.Action("GetCartItems", "Cart")',
                type: 'GET',
                dataType: 'json',
                success: function (response) {
                    console.log('=== CART ITEMS DEBUG ===');
                    console.log('Full response:', JSON.stringify(response, null, 2));
                    console.log('Response type:', typeof response);
                    console.log('Items type:', typeof response.items);
                    console.log('Items value:', response.items);
                    console.log('Items length:', response.items ? response.items.length : 0);
                    if (response.debug) {
                        console.log('Debug info:', response.debug);
                    }
                    
                    var cartList = $('#cart-items-list');
                    cartList.empty();
                    
                    // Xử lý cả camelCase và PascalCase
                    var items = response.items || response.Items || [];
                    var totalAmount = parseFloat(response.totalAmount || response.TotalAmount || 0);
                    
                    console.log('Processed items:', items);
                    console.log('Items is array:', Array.isArray(items));
                    console.log('Total amount:', totalAmount);
                    
                    if (Array.isArray(items) && items.length > 0) {
                        items.forEach(function(item) {
                            console.log('Processing item:', item);
                            // Xử lý cả camelCase và PascalCase
                            var bookId = item.bookId || item.BookId || 0;
                            var title = item.title || item.Title || 'Không có tiêu đề';
                            var authorNames = item.authorNames || item.AuthorNames || 'Updating...';
                            var coverImageUrl = item.coverImageUrl || item.CoverImageUrl || '~/images/books-media/gird-view/book-media-grid-01.jpg';
                            var dailyRate = parseFloat(item.dailyRate || item.DailyRate || 0);
                            var quantity = item.quantity || item.Quantity || 1;
                            
                            var imageUrl = '@Url.Content("~/")' + coverImageUrl.replace('~/', '');
                            var itemHtml = '<li class="clearfix" data-book-id="' + bookId + '">' +
                                '<img src="' + imageUrl + '" alt="' + title + '" style="width: 64px; height: 80px; object-fit: cover;" onerror="this.src=\'@Url.Content("~/images/books-media/gird-view/book-media-grid-01.jpg")\'" />' +
                                '<div class="item-info">' +
                                '<div class="name">' +
                                '<a href="@Url.Action("Detail", "Book")?id=' + bookId + '">' + title + '</a>' +
                                '</div>' +
                                '<div class="author"><strong>Tác giả:</strong> ' + authorNames + '</div>' +
                                '<div class="price">' + quantity + ' X ' + dailyRate.toLocaleString("vi-VN") + ' đ</div>' +
                                '</div>' +
                                '<a class="remove" href="javascript:void(0)" onclick="removeFromCartDropdown(' + bookId + ')"><i class="fa fa-trash-o"></i></a>' +
                                '</li>';
                            cartList.append(itemHtml);
                        });
                        
                        // Cập nhật tổng tiền
                        $('#cart-dropdown-total').text(totalAmount.toLocaleString("vi-VN") + ' đ');
                    } else {
                        console.log('Cart is empty or items is not an array');
                        cartList.html('<li class="clearfix text-center" style="padding: 40px;"><p>Giỏ hàng trống</p></li>');
                        $('#cart-dropdown-total').text('0 đ');
                    }
                },
                error: function (xhr, status, error) {
                    console.error('Lỗi khi load giỏ hàng:', error);
                    console.error('Status:', status);
                    console.error('Response:', xhr.responseText);
                    $('#cart-items-list').html('<li class="clearfix text-center" style="padding: 40px;"><p style="color: red;">Lỗi khi tải giỏ hàng</p></li>');
                }
            });
        }

        // Xóa item khỏi dropdown
        function removeFromCartDropdown(bookId) {
            if (!confirm('Bạn có chắc muốn xóa sách này khỏi giỏ hàng?')) {
                return;
            }

            $.ajax({
                url: '@Url.Action("RemoveFromCart", "Cart")',
                type: 'POST',
                data: { bookId: bookId },
                success: function (response) {
                    if (response.success) {
                        // Xóa item khỏi dropdown
                        $('li[data-book-id="' + bookId + '"]').fadeOut(300, function() {
                            $(this).remove();
                            
                            // Nếu giỏ hàng trống
                            if ($('#cart-items-list li').length === 0) {
                                $('#cart-items-list').html('<li class="clearfix text-center" style="padding: 40px;"><p>Giỏ hàng trống</p></li>');
                            }
                            
                            // Cập nhật số lượng và tổng tiền
                            $('#cart-count-header').text(response.cartCount || 0);
                            var totalAmount = parseFloat(response.totalAmount) || 0;
                            $('#cart-dropdown-total').text(totalAmount.toLocaleString("vi-VN") + ' đ');
                            
                            toastr.success('Đã xóa khỏi giỏ hàng');
                        });
                    }
                },
                error: function () {
                    toastr.error('Có lỗi xảy ra khi xóa khỏi giỏ hàng');
                }
            });
        }

        // Đảm bảo các hàm là global
        window.loadCartItems = loadCartItems;
        window.updateCartCount = function(count) {
            $('#cart-count-header').text(count || 0);
            // Luôn reload cart items khi cập nhật số lượng
            loadCartItems();
        };

        // Load cart count và items khi trang load
        $(document).ready(function() {
            // Load cart count
            $.ajax({
                url: '@Url.Action("GetCartCount", "Cart")',
                type: 'GET',
                success: function (response) {
                    $('#cart-count-header').text(response.cartCount || 0);
                }
            });

            // Load cart items ngay khi trang load
            loadCartItems();

            // Load cart items khi dropdown được mở (Bootstrap 3)
            $('#header-cart-dropdown').on('show.bs.dropdown', function () {
                loadCartItems();
            });
            
            // Cũng load khi dropdown đã mở (fallback)
            $('#header-cart-dropdown').on('shown.bs.dropdown', function () {
                loadCartItems();
            });

            // Load số thông báo chưa đọc
            function loadNotificationCount() {
                $.ajax({
                    url: '@Url.Action("GetUnreadCount", "Notification")',
                    type: 'GET',
                    dataType: 'json',
                    success: function(response) {
                        if (response && response.count > 0) {
                            $('#notification-badge').text(response.count).show();
                        } else {
                            $('#notification-badge').hide();
                        }
                    }
                });
            }

            // Load notification count khi trang load
            @if (!string.IsNullOrEmpty(userId))
            {
                <text>loadNotificationCount();</text>
            }

            // Reload notification count mỗi 30 giây
            setInterval(function() {
                @if (!string.IsNullOrEmpty(userId))
                {
                    <text>loadNotificationCount();</text>
                }
            }, 30000);
        });
    </script>

</body>


</html>